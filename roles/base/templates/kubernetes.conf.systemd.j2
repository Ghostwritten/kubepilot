---
# roles/base/templates/kubernetes.conf.systemd.j2
# Systemd configuration for Kubernetes
# Generated by Ansible - Do not edit manually

[Manager]
DefaultLimitNOFILE={{ system_limits | selectattr('item', 'equalto', 'nofile') | selectattr('type', 'equalto', 'hard') | map(attribute='value') | first | default('65536') }}
DefaultLimitNPROC={{ system_limits | selectattr('item', 'equalto', 'nproc') | selectattr('type', 'equalto', 'hard') | map(attribute='value') | first | default('32768') }}
DefaultLimitCORE=infinity
